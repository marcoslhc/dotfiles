# This tmux statusbar config was created by tmuxline.vim
# on Wed, 03 Oct 2018

set -g status-justify "left"
set -g status "on"
set -g status-attr "none"
set -g message-command-bg "colour0"
set -g status-left-length "100"
set -g pane-active-border-fg "colour114"
set -g status-bg "colour0"
set -g message-command-fg "colour114"
set -g pane-border-fg "colour0"
set -g message-bg "colour0"
set -g status-left-attr "none"
set -g status-right-attr "none"
set -g status-right-length "100"
set -g message-fg "colour114"
setw -g window-status-fg "colour15"
setw -g window-status-attr "none"
setw -g window-status-activity-bg "colour0"
setw -g window-status-activity-attr "none"
setw -g window-status-activity-fg "colour114"
setw -g window-status-separator ""
setw -g window-status-bg "colour0"
set -g status-left "#[fg=colour0,bg=colour114] #S #[fg=colour114,bg=colour0,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=colour0,bg=colour0,nobold,nounderscore,noitalics]#[fg=colour114,bg=colour0] %Y-%m-%d  %H:%M #[fg=colour114,bg=colour0,nobold,nounderscore,noitalics]#[fg=colour0,bg=colour114] #h "
setw -g window-status-format "#[fg=colour15,bg=colour0] #I #[fg=colour15,bg=colour0] #W "
setw -g window-status-current-format "#[fg=colour0,bg=colour0,nobold,nounderscore,noitalics]#[fg=colour114,bg=colour0] #I #[fg=colour114,bg=colour0] #W #[fg=colour0,bg=colour0,nobold,nounderscore,noitalics]"
